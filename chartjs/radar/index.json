        {
          "type": "TEXTINPUT",
          "id": "ymax",
          "label": "Maximum"
        },
        {
          "type": "INTERVAL",
          "id": "yticks",
          "label": "Maximum Number of Ticks"
        },
        {
          "type": "CHECKBOX",
          "id": "ylabels",
          "label": "Hide Tick Labels",
          "defaultValue": "false"
        },
        {
          "type": "CHECKBOX",
          "id": "pointLabels",
          "label": "Hide Axis Labels",
          "defaultValue": "false"
        },
        {
          "type": "CHECKBOX",
