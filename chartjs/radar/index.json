      "elements": [
        {
          "type": "CHECKBOX",
          "id": "legend",
          "label": "Hide Legend",
          "defaultValue": "false"
        },
        {
          "type": "SELECT_SINGLE",
          "id": "legendPosition",
          "label": "Legend Position",
          "defaultValue": "top",
          "options": [{
            "label": "Top",
            "value": "top"
