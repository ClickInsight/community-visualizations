{
  "data": [
    {
      "id": "dimension",
      "label": "Dimensions",
      "elements": [
        {
          "id": "breakdown",
          "label": "Breakdown Dimension",
          "type": "DIMENSION",
          "options": {
            "min": 1,
            "max": 1
          }
        }
      ]
    },
    {
      "id": "metric",
      "label": "Metrics",
      "elements": [
        {
          "id": "value",
          "label": "Value",
          "type": "METRIC",
          "options": {
            "min": 3,
            "max": 20
          }
        }
      ]
    }
  ],
  "style": [
    {
      "id": "chartOptions",
      "label": "Chart Options",
      "elements": [
        {
          "type": "INTERVAL",
          "id": "width",
          "label": "Line Width",
          "defaultValue": "3"
        },
        {
          "type": "CHECKBOX",
          "id": "fill",
          "label": "Fill Area",
          "defaultValue": "true"
        },
        {
          "type": "OPACITY",
          "id": "opacity",
          "label": "Fill Opacity",
          "defaultValue": "0.1"
        }
      ]
    },
    {
      "id": "axisOptions",
