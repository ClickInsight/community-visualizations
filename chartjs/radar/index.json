          "label": "Hide Axis Lines",
          "defaultValue": "false"
        },
        {
          "type": "CHECKBOX",
          "id": "gridlines",
          "label": "Hide Gridlines",
          "defaultValue": "false"
        },
        {
          "type": "CHECKBOX",
          "id": "circular",
          "label": "Show Circular Gridlines",
          "defaultValue": "false"
        }
